package k;

public class asdasd {

	public static void main(String[] args) {
		
		String word = "RARAR";
		 int length = word.length();
		    String out;
		    if(length % 2 == 0){
		      out = word.charAt(length/2);
		      out += word.charAt((length/2)+1);
		    }
		    else{
		      out = word.charAt((length/2)+1);
		    }
		    
		    return out;
		  }

	}

}
