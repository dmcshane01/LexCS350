package k;

public class asdasd {

	public static String main(String[] args) {
		
		System.out.println(middle("TEST"));

	}

	public static String middle(String word)
	{
		
		 int length = word.length();
		    String out;
		    if(length % 2 == 0){
		      out = Character.toString(word.charAt(length/2)) + Character.toString(word.charAt((length/2)+1)) ;
		      out += word.charAt((length/2)+1);
		    }
		    else{
		      out = Character.toString(word.charAt((length/2)+1));
		    }
		    
		    return out;
		  }
	}


